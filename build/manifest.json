{
  "name": "Mino - FPC",
  "version": "0.2.3",
  "manifest_version": 2,
  "description": "a cool new tab extension for FPC",
  "icons": {
    "16": "./static/icons/icon-16.png",
    "128": "./static/icons/icon-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "browser_action": {
    "default_icon": "./static/icons/icon-16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "chrome://favicon/",
    "downloads",
    "storage",
    "https://*.bing.com/",
    "https://*.microsoft.com",
    "https://*.google.com/",
    "https://douban.lovemefan.top/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
